{
  "name": "DevToolBox Configuration",
  "version": "1.0.0",
  "description": "开发者工具箱配置文件",
  "frontend": {
    "redisManager": {
      "apiBaseUrl": "http://localhost:11367",
      "timeout": 5000,
      "retryAttempts": 3
    }
  },
  "backend": {
    "redis": {
      "port": 11367,
      "host": "localhost",
      "logLevel": "info",
      "corsEnabled": true,
      "configDir": "data/redis",
      "allowedOrigins": [
        "chrome-extension://",
        "http://localhost",
        "http://127.0.0.1",
        "https://localhost",
        "https://127.0.0.1"
      ]
    }
  }
}