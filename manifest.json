{
  "manifest_version": 3,
  "name": "DevToolkit",
  "version": "1.0",
  "description": "开发者工具集合",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "clipboardWrite",
    "clipboardRead"
  ]
}